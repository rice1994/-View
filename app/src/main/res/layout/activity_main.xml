<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	tools:context="com.example.lenovo.mpplication.MainActivity">

	<Button
		android:id="@+id/canvas_image_text"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Canvas之图片文字"/>

	<Button
		android:id="@+id/basic_operation_of_Path"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Path之基本操作"/>

	<Button
		android:id="@+id/setPathEffect"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="路径效果setPathEffect"/>

	<com.example.lenovo.mpplication.view.CheckView
		android:id="@+id/cv"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:visibility="gone"/>

</LinearLayout>
