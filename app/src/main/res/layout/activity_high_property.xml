<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	tools:context=".animation.propoty.HighPropertyActivity">

	<Button
		android:id="@+id/ValueAnimator"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="100dp"
		android:text="ValueAnimator"
		android:textAllCaps="false"/>

	<Button
		android:id="@+id/ObjectAnimator"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="ObjectAnimator"
		android:textAllCaps="false"/>
	<com.example.lenovo.mpplication.animation.propoty.MyAnimView
		android:id="@+id/MyAnimView"
		android:layout_width="match_parent"
		android:layout_height="match_parent"/>
</LinearLayout>